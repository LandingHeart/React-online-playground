{
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@3.12"
    }
  },
  "routes": [
    {
      "src": "/api/py/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/docs",
      "dest": "/api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
